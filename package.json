{
  "name": "verse",
  "displayName": "Verse",
  "description": "Syntax support for Epic Games' Verse programming language",
  "version": "1.1.0",
  "icon": "images/unreal-logo.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "publisher": "Worlds",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Manifest-Git/verse-extension"
  },
  "contributes": {
    "languages": [{
      "id": "verse",
      "aliases": ["Verse", "verse"],
      "extensions": [".verse"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "verse",
      "scopeName": "source.verse",
      "path": "./syntaxes/verse.tmLanguage.json"
    }]
  }
}
